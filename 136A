#include <iostream>
using namespace std;
int main()
{
    int n;
    int *p;
    cin>>n;
    p = new int[n+1];
    for ( int i=1;i<=n;i++)
    {
        cin>>p[i];
    }
    for(int j=1;j<=n;j++)
    {
        for(int k=1;k<=n;k++)
        {
            if(p[k]==j)
            {
                cout<<k<<"  ";
                break;
            }
        }
    }
    return 0;
}
