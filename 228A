#include <iostream>
using namespace std;

int main()
{
	int s1,s2,s3,s4,c=0;
	cin>>s1>>s2>>s3>>s4;
	if(s1 == s2 )
		s1=0;
	if(s1 == s3)
		s1=0;
	if(s1 == s4)
		s1=0;
	if(s2 == s3)
		s2=0;
	if(s2 == s4)
		s2=0;
	if(s3 == s4)
		s3=0;
	if(s1==0)
		c++;
	if(s2==0)
		c++;
	if(s3==0)
		c++;
	cout<<c;
    return 0;
}
