#include <iostream>
#include <string>
using namespace std;

int main()
{
    string s;
    int i=0;
    cin>>s;
    do
    {
        if(s[i]=='1' && s[i+1]=='4' && s[i+2]=='4')
            i+=3;
        else if(s[i]=='1' && s[i+1]=='4')
            i+=2;
        else if(s[i]=='1')
            i+=1;
        else{
            cout<<"NO";
            return 0;
        }
    }while(i<s.length());
    cout<<"YES";
}
